{
  "manifest_version": 3,
  "name": "BannApps Tracker",
  "description": "A Chrome extension by Banning Applications",
  "version": "1.0",
  "action": {
    "default_title": "Tracker",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [],
  "host_permissions": [
    "https://bannapps-tracker-api.netlify.app/"
  ]
}